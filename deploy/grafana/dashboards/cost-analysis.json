{
  "id": null,
  "uid": "aeterna-cost-analysis",
  "title": "Aeterna - Cost Analysis",
  "tags": ["aeterna", "cost", "billing"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5m",
  "time": { "from": "now-30d", "to": "now" },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Cost (30d, USD)",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(increase(aeterna_cost_usd_total[30d]))",
          "legendFormat": "total"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Cost Today (USD)",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(increase(aeterna_cost_usd_total[24h]))",
          "legendFormat": "today"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Tenants Over Budget",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "count(aeterna_budget_used_percent > 100)",
          "legendFormat": "over budget"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Avg Budget Utilization",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "avg(aeterna_budget_used_percent)",
          "legendFormat": "avg %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 90 }
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Daily Cost by Resource Type",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(increase(aeterna_cost_usd_total[1d])) by (resource_type)",
          "legendFormat": "{{resource_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD" }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Budget Utilization by Tenant",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "topk(5, aeterna_budget_used_percent)",
          "legendFormat": "{{tenant_id}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      }
    },
    {
      "id": 7,
      "type": "piechart",
      "title": "Cost Distribution by Resource Type (30d)",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(increase(aeterna_cost_usd_total[30d])) by (resource_type)",
          "legendFormat": "{{resource_type}}",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD" }
      }
    },
    {
      "id": 8,
      "type": "table",
      "title": "Top Tenants by Cost (30d)",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "topk(10, sum(increase(aeterna_cost_usd_total[30d])) by (tenant_id))",
          "legendFormat": "{{tenant_id}}",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "sortBy", "options": { "fields": [{ "displayName": "Value", "desc": true }] } }
      ],
      "fieldConfig": {
        "defaults": { "unit": "currencyUSD" }
      }
    },
    {
      "id": 9,
      "type": "table",
      "title": "Budget Alert Status by Tenant",
      "gridPos": { "x": 0, "y": 20, "w": 24, "h": 8 },
      "targets": [
        {
          "expr": "aeterna_budget_used_percent",
          "legendFormat": "{{tenant_id}}",
          "instant": true
        }
      ],
      "transformations": [
        { "id": "sortBy", "options": { "fields": [{ "displayName": "Value", "desc": true }] } }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "color": { "mode": "thresholds" },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "orange", "value": 90 },
              { "color": "red", "value": 100 }
            ]
          }
        }
      }
    }
  ]
}
